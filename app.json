{
  "expo": {
    "name": "pokemon-explore",
    "slug": "pokemon-explore",
    "scheme": ["pokemon-explore", "mycoolredirect"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-pokemon-explore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jhonattancosta.pokemonexplore"
    },
    "web": {
      "bundle": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f1644f0e-bed1-4d4c-905f-464954a852c7"
      }
    }
  }
}
